    <!-- Favicon-->
    <link rel="icon" href="favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="/assets/plugins/bootstrap/css/bootstrap.min.css">

    <link rel="stylesheet" href="assets/plugins/footable-bootstrap/css/footable.bootstrap.min.css">
    <link rel="stylesheet" href="assets/plugins/footable-bootstrap/css/footable.standalone.min.css">
    <!-- Custom Css -->
    <link rel="stylesheet" href="/assets/css/style.min.css">
    <script src="https://cdn.jsdelivr.net/npm/vue"></script>
    <%- include('../includes/loader-style.ejs') %>
</head>

<body class="theme-blue right_icon_toggle">

    <!-- Page Loader -->
    <div class="LW">
        <div class="L">Loading...</div>
    </div>

    <!-- Left Sidebar -->
    <%- include('../includes/side-panel.ejs') %>

        <!-- Main Content -->
        <section class="content">
            <div class="body_scroll">
                <div class="block-header">
                    <div class="row">
                        <div class="col-lg-7 col-md-6 col-sm-12">
                            <h2>feedback Page</h2>
                            <ul class="breadcrumb">
                                <li class="breadcrumb-item"><a href="/"><i class="zmdi zmdi-home"></i> Admin Panel</a></li>
                                <li class="breadcrumb-item active">Pages</li>
                                <li class="breadcrumb-item active">Feedback Page</li>
                            </ul>
                            <button class="btn btn-primary btn-icon mobile_menu" type="button"><i class="zmdi zmdi-sort-amount-desc"></i></button>
                        </div>
                    </div>
                </div>
                <div id="app" class="container-fluid">
                    <div class="row clearfix">
                        <div class="col-12">
                            <div class="card">
                                <div class="header">
                                    <h2><strong>View</strong> Feedbacks</h2>
                                </div>
                                <td class="body">
                                    <td class="row clearfix">
                                        <td class="col-lg-12">
                                            <td class="card">
                                                <td class="table-responsive">
                                                    <table class="table table-hover product_item_list c_table theme-color mb-0">
                                                        <thead>
                                                            <tr>
                                                                <th>#</th>
                                                                <th>Name</th>
                                                                <th>Date</th>
                                                                <th data-breakpoints="sm xs">Feedback</th>
                                                                <th data-breakpoints="sm xs md">Action</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr>
                                                                <td><img src="/assets/images/ecommerce/1.png" width="48" alt="Product img"></td>
                                                                <td>
                                                                    <h5>Simple Black Clock</h5>
                                                                </td>
                                                                <td>
                                                                    <p class="text-muted">randomised words even slightly believable 52364185</p>
                                                                </td>
                                                                <td>$16.00</td>
                                                                <td>
                                                                    <a href="javascript:void(0);" class="btn btn-default waves-effect waves-float btn-sm waves-green"><i class="zmdi zmdi-mail-reply"></i></a>
                                                                    <a href="javascript:void(0);" class="btn btn-default waves-effect waves-float btn-sm waves-red"><i class="zmdi zmdi-delete"></i></a>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </td>
                                            </td>
                                        </td>
                                    </td>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            </div>
        </section>
        <!-- Jquery Core Js -->
        <%- include('../includes/bottom-scripts.ejs') %>
            <script src="/ajax/feedback.js"></script>
